{
    "manifestVersion": 5,
    "id": "Quicker",
    "name": "Quicker",
    "version": "0.9.0",
    "main": "index.html",
    "host": {
        "app": "PS",
        "minVersion": "23.3.0"
    },
    "entrypoints": [
        {
            "type": "command",
            "id": "about",
            "label": {
                "default": "About Quicker...",
                "ja": "Quicker について..."
            }
        },
        {
            "type": "panel",
            "id": "Quicker",
            "label": {
                "default": "Quicker"
            },
            "minimumSize": {
                "width": 240,
                "height": 90
            },
            "maximumSize": {
                "width": 240,
                "height": 400
            },
            "preferredDockedSize": {
                "width": 240,
                "height": 400
            },
            "preferredFloatingSize": {
                "width": 240,
                "height": 400
            },
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/icon_dark.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "darkest", "dark", "medium" ]
                },
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/icon_light.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "lightest", "light" ]
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icons/icon_dark_Plugin.png",
            "scale": [ 1, 2 ],
            "theme": [ "dark", "darkest" ],
            "species": [ "generic" ]
        },
        {
            "width": 23,
            "height": 23,
            "path": "icons/icon_light_Plugin.png",
            "scale": [ 1, 2 ],
            "theme": [ "lightest", "light" ],
            "species": [ "generic" ]
        }
    ],
    "requiredPermissions": {
        "launchProcess": {
            "schemes": [ "https" ]
        }
    }
}
